# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  iray_config = debug

else ifeq ($(config),release)
  iray_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := iray

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

iray:
ifneq (,$(iray_config))
	@echo "==== Building iray ($(iray_config)) ===="
	@${MAKE} --no-print-directory -C . -f iray.make config=$(iray_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f iray.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   iray"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"